  "version": "0.2.0",
  "configurations": [
    {
      "name": "🚀 launch server",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}\\manage.py",
      "args": ["runserver"],
      "django": true,
      "autoStartBrowser": false
    },
    {
      "name": "🏗️ make:migrations",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}\\manage.py",
      "args": ["makemigrations"],
      "django": true,
      "autoStartBrowser": false
    },
    {
      "name": "💾 migrate",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}\\manage.py",
      "args": ["migrate"],
      "django": true,
      "autoStartBrowser": false
    }
  ]
}
